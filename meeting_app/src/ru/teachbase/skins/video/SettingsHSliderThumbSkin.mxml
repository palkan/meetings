        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
	
	<s:BitmapImage source="{style('video','settingsScrollerThumbLeft')}" height="19" width="10"/>
	
</s:SparkSkin>
