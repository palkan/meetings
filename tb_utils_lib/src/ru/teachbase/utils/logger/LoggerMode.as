package ru.teachbase.utils.logger {
public class LoggerMode {
    /** Constant defining JS console output. **/
    public static const CONSOLE:String = "console";
    /** Constant defining there's no output. **/
    public static const NONE:String = "none";
    /** Constant defining the Flash tracing output type. **/
    public static const TRACE:String = "trace";
    /** Constant defining that log is dispatched as events. **/
    public static const EVENT:String = "event";

}
}
